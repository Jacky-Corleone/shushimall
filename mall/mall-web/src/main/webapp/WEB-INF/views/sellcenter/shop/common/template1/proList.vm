<script type="text/javascript">
	$(function(){
		//推荐位弹框事件绑定
		$("p[id*='product']").unbind("click");
		$("p[id*='product']").bind("click",function(){
			var eleId = $(this).attr("id");//遮罩层元素id
			$("#productDialog_eleId").val(eleId);
            $("#_itemName,#_itemId").val("");//清空商品编码和商品名称
			ShopDecorate.buildDecorate("productDialog",eleId);
			ShopDecorate.loadShopProduct();
		});
		
		//清空按钮操作
		$("p[id*='reset_product']").unbind("click");
		$("p[id*='reset_product']").bind("click",function(){
			var product=this.id.split("_")[1];	//获取 字符串 如 "product6"
			var position=this.id.substring(this.id.length-2); // 获取位置信息 如"5f"
			var option = {};
			//shopRenovationList 除了id和位置信息 ，其余全设为空
			option.data = {
					"shopRenovationList[0].id":$("#"+product+"_id").val(),
	                "shopRenovationList[0].pictureUrl":"",
	                "shopRenovationList[0].modultType":position.substring(0,1),
	                "shopRenovationList[0].position":position.substring(1,2),
	                "shopRenovationList[0].chainUrl":"",
	                "shopRenovationList[0].hasPrice":"",
	                "shopRenovationList[0].skuId":"",
	                "shopRenovationList[0].price":"",
	                "shopRenovationList[0].moduleName":"",
	                "templatesId":$("#templatesId").val()
	            }
			$.ajax({
		        type: "POST",
		        url: ctx + "/shopItemListIndexController/temporaryReset",
		        timeout: 100000,
		        data: option.data,
		        dataType:"json",
		        success: function(response){
		            if(response.result=='success'){
		            	//置为空
		               	$("#"+product+"_picUrl").val("");
		               	$("#"+product+"_chainUrl").val("");
		               	$("#"+product+"_pictureUrlName").removeAttr("src");
		               	$("#"+product+"_price").text("￥");
		               	$("#"+product+"_moduleName").text("");
		            }
		        },
		        error: function(){
		            alert("操作出错，请重新操作");
		        }
		    });
		});
		
	});
</script>                    