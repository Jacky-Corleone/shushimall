//发送短信验证码
function sendCode(){
	var address = "$!userInfoDTO.userDTO.umobile";
	var type = 2;
	$.ajax({
		type: "get",
		dataType: "json",
		url:"$request.contextPath/captcha/send",
		data:{
			address:address,
			type:type,
			smsType:'UPGOLDACCOUNT'
		},
		success: function(result){
			if(result.isOK == "1"){
				$("#code_key").val('');
				if(result.message!=''){
					alert(result.message);
				}else{
					alert("验证码发送失败");
					}
			}else{
				$("#code_key").val(result.codeKey);
				$("#sendPhoneLi button").remove();
				$("#mobileSpan").before("验证码发送成功&nbsp;&nbsp;");
			}
		}
	});
}

