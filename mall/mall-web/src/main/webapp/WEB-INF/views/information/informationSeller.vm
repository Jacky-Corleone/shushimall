	function checkCode(codeVal)
	{
		$.ajax({
			async:false,
			type: "post",
			dataType: "json",
			url:"$request.contextPath/captcha/check",
			data:{
				codeKey:$('#code_key').val(),
				code:codeVal
			},
			success: function(result){
				//返回提示信息
				if(result.message)
				{
					$('#checked').val('true');
				}else
				{
					$('#checked').val('');
				}
			}
		});

		return !!$('#checked').val();
	}

