<script type="text/javascript">
$(function() {
	$(".describe img").delayLoading({
		defaultImg: "",    // 预加载前显示的图片
		errorImg: "",                        // 读取图片错误时替换图片(默认：与defaultImg一样)
		imgSrcAttr: "originalSrc",           // 记录图片路径的属性(默认：originalSrc，页面img的src属性也要替换为originalSrc)
		beforehand: 0,                       // 预先提前多少像素加载图片(默认：0)
		event: "scroll",                     // 触发加载图片事件(默认：scroll)
		duration: "normal",                  // 三种预定淡出(入)速度之一的字符串("slow", "normal", or "fast")或表示动画时长的毫秒数值(如：1000),默认:"normal"
		container: window,                   // 对象加载的位置容器(默认：window)
		success: function (imgObj) { },      // 加载图片成功后的回调函数(默认：不执行任何操作)
		error: function (imgObj) { }         // 加载图片失败后的回调函数(默认：不执行任何操作)
	});
}); 
	var loadingImg = "<div style='height: 42px; line-height: 42px; text-align: center;'><img src='$assetsPath/images/loading-1j.gif'></div>";
	// 页面初始化、改变sku_id
	function flushCommCount() {
		var param = {};
		if ($.trim( $("#skuId").val() )) {
			param.skuId = $("#skuId").val();
		} else {
			param.itemId = $("#itemId").val();
		}
		$.post(
			'$request.contextPath/productController/getItemEvaluationCount',
			param,
			function(data) {
				data.success && $("#commCount").text(data.count);
			},
			'json'
		);
	}
	
	// myTab0样式更新，显示激活区块
	function active_myTab0(obj, index) {
    	// li的样式更改
    	if ($(obj).hasClass('active')) return;
    	$(obj).removeClass('normal').addClass('active')
    		.siblings('.active').removeClass('active').addClass('normal');
    	
    	// 显示对应的div块
    	var refContentDiv = $("#myContent0 > div:eq(" + index + ")");
    	refContentDiv.show().siblings().hide();
	}
	
				
	// 请求所需要的参数，返回itemId或skuId
	function getItemOrSku() {
		var itemId = $("#itemId").val();
		var skuId = $("#skuId").val();
		if (skuId) return {skuId: skuId};
		return {itemId: itemId};
	}
	
	// 评价内容检索
	function evaluationContentSearch(page) {
		$("#tabContent").html(loadingImg);
		var reqParam = getItemOrSku();
		if ($("#hasShow").val()=="1") reqParam.hasShow = 1;
		if (page) reqParam.page = page;
		$("#tabContent").load(
			'$request.contextPath/productController/getItemEvaluation',
			reqParam,
			function() {
				// 晒图可以放大查看
				$("img[fancy_zoom='view_img']").fancyzoom({
            		Speed:400,
            		showoverlay:false,
            		imgDir:'$request.getContextPath()/assets/plugin/fancyzoom/ressources/'
            	});
			}
		);
	}
	
	$(function() {
		
		flushCommCount();
		
		/*
		详情介绍、规格参数、用户评价、服务支持这一行的li，绑定点击事件
		除了<li>用户评价</li>这一项，其他的都是简单的改变样式和显示对应的div块
		*/
		$('#myTab0 li').each(function (index) {
			var id = this.id;
			// 点击用户评价
			if (id == 'userCommTab') {
				$(this).click(function () {
					var obj = this;
    				if (!$.trim( $("#commWrapper").html() ) || isSkuChange()) {
						$("#commWrapper").html(loadingImg);
						active_myTab0(obj, index);
						var reqParam = getItemOrSku();
    					// 加载评价页头部
    					$("#commWrapper").load(
    						'$request.contextPath/productController/getItemEvaluationHead',
    						reqParam,
							function () {
								$("#commCount2").text($("#commCount").text());
								
								// 全部评价、有图评价绑定点击事件
								$("#Tab0 li").each(function(index) {
									$(this).click(function() {
										$("#hasShow").val(index);
										if ($(this).hasClass('active')) return;
                                    	$(this).removeClass('normal').addClass('active')
                                    		.siblings('.active').removeClass('active').addClass('normal');
										evaluationContentSearch();
									});
								});
								
								// 加载页面评价内容
								evaluationContentSearch();
							}
    					);
    				} else {
						active_myTab0(this, index);
					}
				});
				
				// 判断sku是否切换过
				function isSkuChange() {
					var pageSku = +$("#skuId").val();
					return pageSku != comm_skuId;
				}
			} else {
				$(this).click(function () {
					active_myTab0(this, index);
				});
			}
		});
	});



	function loadEvaluation(page){
		$("#myTab0_Content2").html("<div style='height: 42px; line-height: 42px; text-align: center;'><img src='$assetsPath/images/loading-1j.gif'></div>");
		var skuId = $("#skuId").val();
		var itemId = $("#itemId").val();
		$.ajax({
			url: '$request.contextPath/productController/getItemEvaluation',
			data: { "page": page,"skuId":skuId,"itemId":itemId },
			type: 'post',
			dataType: "html",
			success: function(data){
				$("#myTab0_Content2").html(data);
				$("img[fancy_zoom='view_img']").fancyzoom({
            		Speed:400,
            		showoverlay:false,
            		imgDir:'$request.getContextPath()/assets/plugin/fancyzoom/ressources/'
            	});
			}
		});
	};
    $(document).ready(function(){
        //loadEvaluation(0);
    });

    function openCustomerWin(){
        var retUrl = window.location.href;
        retUrl = retUrl.replace(window.location.host,'').replace('http://','').replace('/mall-web','');
        this.location.href="$request.contextPath/user/login?retUrl="+retUrl;
        return false;
    }
