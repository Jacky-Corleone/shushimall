	function setCentralPurchasingState(o,activitesDetailsId){
		window.location.reload();
	}
	
	$(document).ready(function(){
		$(".mc_prompt_close").click(function(){
			$(this).parents(".po_fai").fadeOut();
		});
	
		$(".cou_img").click(function(e){
	        $(this).next().toggle()
	        e.stopPropagation();
	        e.cancelBubble=true;
	    });
	    $(".coupon_bg").click(function(e){
	        e.stopPropagation();
	        e.cancelBubble=true;
	    });
	    $(".coupon_close").click(function(){
	        $(".coupon_bg").hide();
	    });
	    $(document).click(function(){
	        $(".coupon_bg").hide();
	    })
	});
	
	//领取优惠券
	function getCoupon(couponId) {
	    if (couponId && '' != $.trim(couponId)) {
	        $.ajax({
	            url: "$request.contextPath/coupons/getCoupons",
	            data: {
	                "couponId": couponId
	            },
	            type: 'post',
	            dataType: 'json',
	            success: function (data) {
	                // 提示信息
	                alert(data.resultMessage);
	
	                // 如果领取成功且满足限领条件后，页面中删除该优惠券
	                var result = data.result;
	                if (result) {
	                    $("li").remove("." + couponId);
	                }
	            }
	        });
	    }
	}
</script>

