<body class="po_re bg_02">
<div id="main" class="po_re">
	<input style="display:none" id="tabs1" type="text"/>
	<input style="display:none" id="tabs2" type="text"/>
	<input style="display:none" id="tabs3" type="text"/>
	<input style="display:none" id="tabs0" type="text"/>
	  #if($sourcePage=="seller")
         <input type="hidden" value="2" id="searchType">
      #elseif($sourcePage=="buyer")
         <input type="hidden" value="3" id="searchType">
      #end
	
    <!-- 中间内容 -->
    <div class="content" id="wrapper1">
        <div class="nTab">
            <!-- 标题开始 -->
            <div class="TabTitle bg_01">
                <ul id="myTab0" class="myTab0">
                    <li class="active" onclick="contractPageTabs(this,0);">协议管理</li>
                    <li class="normal" onclick="contractPageTabs(this,1);">协议创建</li>
                    <li class="normal" onclick="contractPageTabs(this,2);">待审核</li>
                    <li class="normal" onclick="contractPageTabs(this,3);">待确认</li>
                </ul>
            </div>
            <!-- 标题开始 end-->
            <div class="clear"></div>
            <!-- 内容开始 -->
            <div class="TabContent">
                <!-- 切换内容    1   -->
                <div id="myTab0_Content0">
                    <div>
                        <div id="searchForm0" class="bg_01">
                            <form id="contractSearchForm">
                                <ul class="font_t pad_lr5">
                                    <li class="hei_32">
                                        <p class="wid_25 fl font_right pad_r5">
                                            #if($sourcePage=="seller")
                                                买方：
                                            #elseif($sourcePage=="buyer")
                                                卖方：
                                            #end
                                        </p>
                                        <input id="companyName" class="form-control wid_50 hei_20" maxlength="45"
                                               type="text" name="companyName"/>
                                    </li>
                                    <input type="hidden" name="printerId"/>
                                    <input type="hidden" name="supplierId"/>
                                    <li class="hei_32"><p class="wid_25 fl font_right pad_r5">商品名称</p><input
                                            name="itemName" maxlength="45"
                                            class="form-control wid_50 hei_20"/>
                                    </li>
                                    <li class="hei_32"><p class="wid_25 fl font_right pad_r5">协议名称</p><input
                                            name="contractName" maxlength="45"
                                            class="form-control wid_50 hei_20"/>
                                    </li>
                                </ul>
                            </form>
                            <div style="overflow:hidden;" class=" font_cen">

                                <button class="button_1 pad_lr5 hei_32" onclick="searchContractInfoByCondition(0)">
                                    协议查询
                                </button>
                                <button class="button_1 pad_lr5 hei_32" onclick="reset()">
                                    重 置
                                </button>
                            </div>
                        </div>
                    </div>
                  
                    <!--协议遍历-->
                    <div id="contractList">
                        <!-- 表格 -->
                        #foreach($contract in $conInfos)
                            #if($contract.createBy==$register.uid&&($contract.createBy==$contract.printerId||$contract.createBy==$contract.supplierId))
                                <!-- 发货商城 -->
                                <div class="bg_01 pad_mlr5 mar_b10">
                                    <div class="hei_32 border_2">
                                        <p class="fl over_ell"><span>协议名称：$!contract.contractName</span>
                                        </p>

                                        <p class="fr font_e5">$!statusMap.get($contract.status)</p>
                                    </div>
                                    <!-- 发货商城 -->
                                    <!-- 发货信息 -->
                                    <div class="border_2 shop_order pad_tb5">
                                        #foreach($contractMat in $contract.contractMatDTOs)
                                            <div class="wid_50 fl shop_order_p">
                                                <p class="fl over_ell">商品编号：
                                                    <a href="$request.contextPath/product/toDetail?id=$!{contractMat.get('itemId')}&skuId=$!{contractMat.get('skuId')}">
                                                        $!contractMat.get("skuId")
                                                    </a>
                                                </p>
   												<div class="clear"></div>
                                                <p class="fl over_ell">商品名称：
                                                    <a href="$request.contextPath/product/toDetail?id=$!{contractMat.get('itemId')}&skuId=$!{contractMat.get('skuId')}">
                                                        $!contractMat.get("itemName")
                                                    </a>
                                                </p>
												   <div class="clear"></div>
                                                <p class="fl over_ell">商品属性：$!{contractMat.get("salerAttr")}</p>
                                                   <div class="clear"></div>
                                                <p class="fl over_ell">协议类型：
                                                 #if("1"==$!contract.protocolType)
                                                 	单价
                                                 #end
                                                 #if("2"==$!contract.protocolType)
                                                 	数量 ：$!contractMat.get("number")
                                                 #end
                                                 #if("3"==$!contract.protocolType)
                                                 	总价格：￥$!contractMat.get("cost")
                                                 #end
                                                </p>
                                                
                                            </div>
                                            <div class="fr lin_24">
                                                <p>￥$!contractMat.get("matPrice")</p>
                                            </div>
                                        #end
                                        <div class="clear"></div>
                                    </div>
                                    <!-- 发货信息 -->
                                    <p class="hei_34">创建日期：$!date.format('yyyy-MM-dd',$contract.createDate)</p>
