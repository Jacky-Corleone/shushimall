                                    <input type="hidden" name="supplierId" id="partBId">
                                </li>
                            #end

                            <li>
                                <p class="wid_30 fl font_right pad_r5"><span
                                        class="font_e5">*</span>账期</p>

                                <div style="width:60%" class="fl">
                                    <input type="radio" checked="checked" value="0" name="paymentType"
                                           onclick="radioChange()"/>
