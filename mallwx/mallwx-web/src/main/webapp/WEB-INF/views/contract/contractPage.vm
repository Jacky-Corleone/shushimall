                                        <option value="3">总价值</option>
                                    </select>
                                </div>
                            </li>
                            #if($sourcePage=="seller")
                                <li><p class="wid_30 fl font_right pad_r5"><span class="font_e5">*</span>买方</p><input
                                        onclick="getCompany(1,'company')" name="printerName" id="partBName"
