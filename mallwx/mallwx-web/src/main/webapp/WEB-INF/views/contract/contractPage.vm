                                            name="paymentDays">
                                        <option value=""></option>
                                        <option value="7">7</option>
                                        <option value="15">15</option>
                                        <option value="30">30</option>
                                        <option value="45">45</option>
                                        <option value="60">60</option>
                                        <option value="75">75</option>
                                        <option value="90">90</option>
                                        <option value="120">120</option>
                                        <option value="180">180</option>
                                    </select>
                                    <span style="padding-left: 4%;padding-right:4%">天</span>
                                </div>
                            </li>
                            <li>
                                <p class="wid_30 fl font_right pad_r5" style="height: 50px;"><span
                                        class="font_e5"></span></p>

                                <div class="fl" style="width:60%">
                                    <input type="radio" value="1" onclick="radioChange()" name="paymentType"/>
                                    <select name="paymentMonth" id="choseMonth" disabled="true"
                                            class="wid_30 form-control">
                                        <option value=""></option>
                                        <option value="1">1</option>
                                        <option value="2">2</option>
                                        <option value="3">3</option>
                                        <option value="4">4</option>
                                        <option value="5">5</option>
                                        <option value="6">6</option>
                                    </select>
                                    <span style="padding-left: 4%;padding-right:4%">月</span>
                                </div>
                                <!--创建完毕:0;协议需要审批:1;审批完毕:2;确认完毕:4;-->
                            </li>
                        
                            <li id="uploadLi">
                                <p class="wid_30 fl font_right pad_r5">附件</p>
                       
                                <input type="file" class="wid_50 form-control hei_20" id="fileInput" name="file"
                                       onchange="startUpload();"/>
                            </li>
                              
           					<div class="clear"></div>
                            <li><p class="wid_30 fl font_right pad_r5">备注</p><input maxlength="45"
                                                                                    type="text"
                                                                                    class="wid_50 form-control"
                                                                                    name="remark"/></li>
                            <li><p class="wid_30 fl font_right pad_r5">需要审批</p>

                                <p><input type="checkbox" name="status" value="7"
                                          onclick="showApproveList(this)"/>
                                    是</p>
                            </li>

                            <li style="display: none;" id="approveBy">
                                <p class="wid_30 fl font_right pad_r5">选择审批人</p>
                                <select id="approveList" class="wid_55 form-control" name="approveBy" disabled="disabled">
                                	#foreach($childUserListPost in $approveList)
                                		<option value="$childUserListPost.userId">$childUserListPost.username</option>
                                	#end
                                </select>
                            </li>
                            <div class="clear"></div>
                                     <input type="hidden" id="annexcc" name="annexcc"/>
                                       <input type="hidden" id="annex" name="annex"/>
                        </ul>
                    </form>
                    <!-- 表单 end-->
                    <!-- table -->
                    <div>
                        <div id="itemList">
                        </div>
                        <div class="wid_100 hei_32 font_cen"
                             onclick="showItemSearchDIV()">
                            <span>点击新增商品</span><i class="fa fa-plus-circle"></i>
                        </div>

                    </div>
                    <!-- table -->
                    <div class="font_right">
                        <button class="hei_32 pad_lr10 button_1" onclick="addContractInfo()">保存</button>
                    </div>
                    <div class="clear"></div>
                </div>
