    <div class="TabTitle bg_01">
      <ul id="myTab0" class="myTab3">
        <li class="active" onclick="nTabs(this,0);">商品介绍</li>
        <li class="normal" onclick="nTabs(this,1);">包装清单</li>
        <li class="normal" onclick="nTabs(this,2);">售后保障</li>
      </ul>
    </div><!-- 标题开始 end-->
