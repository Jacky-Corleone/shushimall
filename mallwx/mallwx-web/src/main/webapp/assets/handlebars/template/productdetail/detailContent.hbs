<div class="swiper-container" style="height: 250px;">
    <div class="swiper-wrapper" id="swiper-wrapper">
    	{{putGix gix}}
		{{#each item.picUrls}}
        	<div class="swiper-slide"><a href="#"><img src="{{getGix this}}"></a></div>
		{{/each}}
    </div>  
    <div class="pagination"></div> 
</div>  
<div class="clear"></div>
<ul class="goods_Details bg_01">
     <li>
        <p class="lin_24">
            {{item.itemName}} <br/>
            <font class="font_18">这是单品活动，活动活动，活动，活动，人生得意须尽欢，莫使金樽空对月</font>
        </p>
        <p class="hei_34">
			{{#compare item.hasPrice "==" 1}}
            	<font class="font_e3 mar_r2" guidPrice id="itemPrice_js">￥{{item.guidePrice}}</font> 
			{{else}}
				没有价格，需要询价
			{{/compare}}
            <font class="font_e3"><del>￥{{item.marketPrice}}</del></font>
        </p>
    </li>
 </ul>
<div class="clear"></div>
<!-- 选择 -->
<div class="hei_45 pad_lr5 bg_01 border_bt" onclick="show_shop()">
    <span class="fl font_7_5" id="attrShowPad">
    {{#if skuItem}}
    	{{#each skuItem.attrSales}}
    		
    		{{#each values}}
    		"{{name}}"
    		{{/each}};
    	{{/each}}
    {{else}}
    	请选择：
    	{{#each item.attrSale}}
    	{{name}}{{" "}}
    	{{/each}}
    {{/if}}
    </span>
    <span class="fr"><i class="fa fa-angle-right "></i></span>
    <div class="clear"></div>
</div><!-- 选择 end-->
<!-- 评论-->
