#set($layout = "/none_layout.vm")<ul id="detailTable" class="list_top font_cen font_14b">    <li class="wid_150">公司名称</li>    <li class="wid_110">联系人</li>    <li class="wid_120" #if($!uType == 2)style="display:none;"#end>联系方式</li>    <li class="wid_170" #if($!uType == 2)style="display:none;"#end>电子邮件</li></ul>#foreach($userInfo in $pager.records)<ul class="list_top cursor" onclick="getPartBDetail(this)">    <li class="wid_150 over_ell font_cen" title="$!userInfo.companyName">$!userInfo.companyName&nbsp;</li>    <li class="wid_110 over_ell font_cen" title="$!userInfo.uname">$!userInfo.uname</li>    <li class="wid_120 over_ell font_cen" #if($!uType == 2)style="display:none;"#end>$!userInfo.umobile</li>    <li class="wid_170 over_ell font_cen" #if($!uType == 2)style="display:none;"#end>$!userInfo.userEmail</li>    <li style="display:none">$!userInfo.parentId</li>    <input type="hidden" name="conSupplierId" value="$userInfo.uid">    <input type="hidden" name="shopId" value="$userInfo.shopId"></ul>#end<!-- 分页 -->#pageVelocity($pager)