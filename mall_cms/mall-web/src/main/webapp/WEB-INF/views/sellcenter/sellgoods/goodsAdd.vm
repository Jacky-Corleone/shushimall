$(document).ready(function(){
	// 下拉箭头事件
	$(".trigger").click(function(){
		var citys = $(this).parent().next(".citys");
		$(this).parent().parent().toggleClass(function(){
			if($(this).hasClass("showCityPop")){
				citys.hide();
			}else{
				$(".citys").hide();
				$(".ecity").removeClass("showCityPop");
				citys.show();
			}
			return "showCityPop";
		});
	});
	//选择省事件
	$(".J_Province").change(function(){
		var cked = $(this).prop("checked");
		var citys = $(this).parent().next(".citys");
		citys.find(".J_City").prop("checked", cked);
		handleChange();
	});
	//城市选择事件
	$(".J_City").change(function(){
		var gareas = $(this).parent().parent().prev(".gareas");
		handleChange();
	});
	//关闭按钮事件
	$(".close_button").click(function(){
		$(".citys").hide();
		$(".ecity").removeClass("showCityPop");
		//showCityPop
	});
	initProvinceCitys("");
});

//当省市复选框发生改变的时候触发一次
function handleChange(){
	$(".ecity").each(function(){
		var gareas = $(this).children(".gareas");
		var citys = $(this).children(".citys");
		var citysNum = citys.find(".J_City").length;
		var checkedNum = citys.find(".J_City:checked").length; //已经选择数量
		if(citysNum > 0) {
			gareas.find(".J_Province").prop("checked", citysNum == checkedNum); //如果全部选中了则选中省 否则取消
		}
		gareas.find(".check_num").html(checkedNum > 0 ? "("+checkedNum+")" : "");
	});
}

//省市初始化、回显
function initProvinceCitys(ids){
	$(".province-list .citys").hide();
	$(".province-list .ecity").removeClass("showCityPop");
	$(".province-list .check_num").html("");
	$(".province-list input:checkbox").prop("checked", false);
	$(ids.split(",")).each(function(i, id){
		$("#J_City_" + id).prop("checked",true);
	});
	handleChange();
}
