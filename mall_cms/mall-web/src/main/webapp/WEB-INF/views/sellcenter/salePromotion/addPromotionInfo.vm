	function numInput(obj,length){
		if(obj.value==obj.value2)
			return;
		if(length == 0 && obj.value.search(/^\d*$/)==-1)
			obj.value=(obj.value2)?obj.value2:'';
		else if(obj.value.search(/^\d*(?:\.\d{0,2})?$/)==-1)
			obj.value=(obj.value2)?obj.value2:'';
		else obj.value2=obj.value;
	}
	
