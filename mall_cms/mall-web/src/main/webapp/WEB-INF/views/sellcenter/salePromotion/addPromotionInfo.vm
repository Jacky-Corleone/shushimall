	function deletel(obj,itemId){
		$(obj).parent().parent().remove();
		//如果删除所有的阶梯价则同步删除整行sku记录
		var tableObj = $("#areaAllTable_"+itemId).find("tr");
		if(tableObj.length == 1){
			$("#seledPro1_"+itemId).remove();
			$("#selecdR1_"+itemId).prop("checked",false);
			$("#checkedAll1").prop("checked",false);
		}
	}
	function addel(obj){
		var parentObj = $(obj).parent().parent();
		var grandObj = parentObj.parent();
		 //获取需要添加的行数据
		 var tr = parentObj.clone();
		 //将数据存入阶梯价table中
		 grandObj.append(tr);
		 //清空新增行的数据
		 grandObj.find("tr:last input").val("");
	}
