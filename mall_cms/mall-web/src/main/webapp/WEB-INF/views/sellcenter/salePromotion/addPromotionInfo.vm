	// 商品全选/全取消
	function addAll(){
		var flag = $("#checkedAll").prop("checked");
		// 移除已选商品，重新添加
		if(flag){
			$("#productInfo").children().remove();
		}
		// 逐条添加商品
		$("input[name='selectedItem']").each(function(){
			$tr = $(this).parents("tr");
			$itemName = $tr.children("td:eq(3) div:eq(1)").text();
			$skuId = $tr.children("td:eq(2)").text();
			$(this).prop("checked",flag);
			addHtml(this,$skuId,$itemName);
		});
		// 还原全选按钮 
		$("#checkedAll").prop("checked",flag);
	}
