<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        ">
	<!-- 使用DUBBO协议暴露服务;商城广告 -->
	<dubbo:service interface="com.camelot.sellercenter.malladvertise.service.MallAdExportService"
		owner="camelot" ref="mallAdExportService" version="1.0" protocol="dubbo"
		delay="-1" registry="qingdaoRegistry" />
		
	<dubbo:service interface="com.camelot.sellercenter.mallrecattr.service.MallRecAttrExportService"
		owner="camelot" ref="mallRecAttrExportService" version="1.0" protocol="dubbo"
		delay="-1" registry="qingdaoRegistry" />


	<!-- 使用dubbo协议暴露广告点击量服务 -->
	<dubbo:service interface="com.camelot.sellercenter.mallword.service.MallWordExportService" 
 		owner="camelot" ref="mallWordExportService" version="1.0" protocol="dubbo" 
	delay="-1" registry="qingdaoRegistry" /> 
</beans>